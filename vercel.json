{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/_functions/entry",
      "dest": "/_functions/entry"
    },
    {
      "src": "/api/.*",
      "dest": "/api/"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/_functions/entry"
    }
  ]
}